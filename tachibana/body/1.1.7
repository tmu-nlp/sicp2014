sqrtを動かすためには以下の関数が必要となる
(define (abs x)
  (if (< x 0)
      (- x)
      x))


(define (square enix) (* enix enix))

(define
     (average x y)
     (/ (+ x y) 2)
)

(define
     (improve guess x)
     (average guess (/ x guess))
)

(define
     (good-enough? guess x)
     (< (abs (- (square guess) x)) 0.001)
)

(define (sqrt-iter guess x)
  (if (good-enough? guess x)
      guess
      (sqrt-iter (improve guess x)
                 x)))

(define (sqrt x)
  (sqrt-iter 1.0 x))